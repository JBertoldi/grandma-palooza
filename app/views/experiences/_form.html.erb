<%= simple_form_for(experience) do |f| %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :price %>
<%= f.input :duration %>
<%= f.association :grandmother, label_method: :last_name, value_method: :id, include_blank: false, collection: current_user.grandmothers  %>
<%= f.submit %>
<% end %>
